{
  "name": "base_operativo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "clear": "rimraf build/",
    "clean_build": "npm run clear && npm run build",
    "migrate": "npx sequelize db:migrate --options-path ./src/data/postgreSQL/storage/.sequelizerc",
    "seed": "npx sequelize db:seed:all --options-path ./src/data/postgreSQL/storage/.sequelizerc",
    "migrate:generate": "npx sequelize migration:generate --options-path ./src/data/postgreSQL/storage/.sequelizerc --name",
    "seed:generate": "npx sequelize seed:generate --options-path ./src/data/postgreSQL/storage/.sequelizerc --name",
    "reset": "ts-node scripts/clearDB.ts && npm run migrate && npm run seed",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --check .",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:fix": "prettier --write .",
    "prepare": "husky"
  },
  "author": "juandav9312",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "env-var": "^7.4.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "mjml": "^4.14.1",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "ts-node": "^10.9.2",
    "uid-safe": "^2.1.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mjml": "^4.7.4",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "@types/uid-safe": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^9.0.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "sequelize-cli": "^6.6.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3"
  }
}
